{
	"name": "ds_arquivos_ftp",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ftpserver_caged",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ano": {
				"type": "string"
			},
			"competencia": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "FtpServerLocation",
				"fileName": "*.7z",
				"folderPath": {
					"value": "@concat(dataset().ano,'/', dataset().competencia)",
					"type": "Expression"
				}
			}
		}
	}
}